{
  "name": "api-vagas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only ./src/index.ts",
    "postinstall": "tsc",
    "start": "node ./build/index.js",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:create": "typeorm migration:create",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d ./src/main/config/database.config.ts",
    "typeorm:revert": "npm run typeorm migration:revert -- -d ./src/main/config/database.config.ts",
    "typeorm:generate": "typeorm-ts-node-esm migration:generate -d ./src/main/config/database.config.ts src/app/shared/database/migrations/CreateTables",
    "test": "cross-env NODE_ENV=test jest --maxWorkers=1 --silent --noStackTrace",
    "test:verbose": "jest -verbose -runInBand -detectOpenHandles",
    "test:coverage": "jest -runInBand -coverage",
    "test:watch": "jest -silent -noStackTrace -runInBand -watchAll",
    "typeorm:run:test": "cross-env NODE_ENV=test typeorm-ts-node-commonjs migration:run -d ./src/main/config/database.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jest": "^29.6.2",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  }
}
